class Solution {
    public void moveZeroes(int[] nums) {
        int n=nums.length;
        int z=0;
        for(int i=0;i<n;i++)
        {
            if(nums[i]!=0)
            {
                nums[z]=nums[i];
                z++;
            }
        }
        for(int i=z;i<n;i++)
        {
            nums[i]=0;
        }
        
    }
}
